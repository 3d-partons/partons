<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<scenario id="02" date=""
    description="Example : computation of one GPD model (GK11) with evolution (Example)">

    <task service="GPDService" method="computeGPDModelWithEvolution">
        <GPDKinematic>
            <param name="x" value="0.1" />
            <param name="xi" value="0.00050025" />
            <param name="t" value="-0.3" />
            <param name="MuF2" value="8" />
            <param name="MuR2" value="8" />
        </GPDKinematic>

        <GPDModule>
            <param name="id" value="GK11Model" />
        </GPDModule>

        <GPDEvolutionModule>
            <param name="id" value="ExampleEvolQCDModel" />
            <param name="qcd_order_type" value="LO" />
        </GPDEvolutionModule>
    </task>

</scenario>
