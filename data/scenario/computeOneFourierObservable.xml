<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<scenario id="05" date=""
	description="Example : computation of one fourier observable(AcCos2phi) with GV2008 DVCS model">

	<task service="ObservableService" method="computeDVCSObservable">
	       <ScaleModule>
            <param name="id" value="LambdaQ2Scale" />
            <param name="lambda" value="1" />
        </ScaleModule>

        <XiConverterModule>
            <param name="id" value="XBToXi" />
        </XiConverterModule>
        
		<ObservableKinematic>
			<param name="xB" value="0.1763" />
			<param name="t" value="-0.1346" />
			<param name="Q2" value="1.3651" />
		</ObservableKinematic>

		<Observable>
			<param name="id" value="AcCos2phi" />
		</Observable>

		<DVCSModule>
			<param name="id" value="GV2008Model" />
			<param name="beam_energy" value="5.77" />
		</DVCSModule>

		<DVCSConvolCoeffFunctionModule>
			<param name="id" value="DVCSCFFModel" />
			<param name="qcd_order_type" value="LO" />
		</DVCSConvolCoeffFunctionModule>

		<GPDModule>
			<param name="id" value="GK11Model" />
		</GPDModule>
	</task>
</scenario>
