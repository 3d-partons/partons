<scenario id="05" date="" description="Comparison of Alu with LO and NLO.">

  <task service="ObservableService" method="computeManyKinematicOneModel"
    storeInDB="0">
    <ScaleModule>
      <param name="id" value="Q2Multiplier" />
      <param name="lambda" value="1" />
    </ScaleModule>

    <XiConverterModule>
      <param name="id" value="XBToXi" />
    </XiConverterModule>

    <ObservableKinematic>
      <param name="file" value="/home/debian/workspace/PARTONS/data/phi.dat" />
    </ObservableKinematic>

    <Observable>
      <param name="id" value="Alu" />
    </Observable>

    <DVCSModule>
      <param name="id" value="BMJ2012Model" />
      <param name="beam_energy" value="11" />
    </DVCSModule>

    <DVCSConvolCoeffFunctionModule>
      <param name="id" value="DVCSCFFModel" />
      <param name="qcd_order_type" value="LO" />
    </DVCSConvolCoeffFunctionModule>

    <GPDModule>
      <param name="id" value="GK11Model" />
    </GPDModule>
  </task>

  <task service="ObservableService" method="computeManyKinematicOneModel"
    storeInDB="0">
    <ScaleModule>
      <param name="id" value="Q2Multiplier" />
      <param name="lambda" value="1" />
    </ScaleModule>

    <XiConverterModule>
      <param name="id" value="XBToXi" />
    </XiConverterModule>

    <ObservableKinematic>
      <param name="file" value="/home/debian/workspace/PARTONS/data/phi.dat" />
    </ObservableKinematic>

    <Observable>
      <param name="id" value="Alu" />
    </Observable>

    <DVCSModule>
      <param name="id" value="BMJ2012Model" />
      <param name="beam_energy" value="11" />
    </DVCSModule>

    <DVCSConvolCoeffFunctionModule>
      <param name="id" value="DVCSCFFModel" />
      <param name="qcd_order_type" value="NLO" />
    </DVCSConvolCoeffFunctionModule>

    <GPDModule>
      <param name="id" value="GK11Model" />
    </GPDModule>
  </task>
</scenario>
