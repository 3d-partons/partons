<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<scenario id="01" date=""
	description="Example : computation of one GPD model (GK11) without evolution">

	<task service="GPDService" method="computeGPDModel">
		<GPDKinematic>
			<param name="x" value="0.1" />
			<param name="xi" value="1.00050025" />
			<param name="t" value="-0.3" />
			<param name="MuF2" value="8" />
			<param name="MuR2" value="8" />
		</GPDKinematic>

		<GPDModule>
			<param name="id" value="GK11Model" />
		</GPDModule>
	</task>

</scenario>
